<?xml version='1.0' encoding='utf-8'?>
<widget id="com.skubagdeveloper.app" version="3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SKUBAG</name>
    <description>
         A mobile app top buy WAEC scratch cards and GES Placement cards and check wassce results instantly.
    </description>
    <author email="richardneaverscoy@gmail.com" href="#">
         WAEC RESULTS CHECKER
    </author>
    <plugin name="org.apache.cordova.inappbrowser" source="npm" />
    <preference name="CrosswalkAnimatable" value="true" />
    <content src="index.html" />
    <icon src="icon.png"/>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <!--<feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>-->
</widget>
